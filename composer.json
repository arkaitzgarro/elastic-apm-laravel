{
    "name": "arkaitzgarro/elastic-apm-laravel",
    "description": "Laravel APM agent for Elastic v2 intake API",
    "keywords": ["laravel", "elastic", "apm"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arkaitz Garro",
            "email": "arkaitz.garro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/routing": "^5.5|^6",
        "illuminate/support": "^5.5|^6",
        "philkra/elastic-apm-php-agent": "v7.0.0-rc3"
    },
    "autoload": {
        "psr-4": {
            "AG\\ElasticApmLaravel\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "AG\\ElasticApmLaravel\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "5.5.x"
    }
}
